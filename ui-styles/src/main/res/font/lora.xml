<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <font
        app:font="@font/lora_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        android:font="@font/lora_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        />
    <font
        app:font="@font/lora_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        android:font="@font/lora_italic"
        android:fontStyle="italic"
        android:fontWeight="400"/>
    <font
        app:font="@font/lora_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        android:font="@font/lora_bold"
        android:fontStyle="normal"
        android:fontWeight="700" />
    <font
        app:font="@font/lora_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700"
        android:font="@font/lora_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="700" />
</font-family>
